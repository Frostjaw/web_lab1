@{
    ViewData["Title"] = "PassUsingModel - Backend1";
}
@model Dictionary<string, double>
<h2>PassUsingModel</h2>
<p>Rand Value First: @Model["firstNumber"]</p>
<p>Rand Value Second: @Model["secondNumber"]</p>
<p>Add: @Model["sum"]</p>
<p>Sub: @Model["sub"]</p>
<p>Mult: @Model["mult"]</p>
@if (Model.ContainsKey("div"))
{
    <p>Div: @Model["div"]</p>
}
else
{
    <p>Div: Деление на 0</p>
}
